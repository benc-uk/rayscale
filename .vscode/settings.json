{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.format.enable": true,
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "eslint.workingDirectories": [
        "./tracer",
        "./controller",
        "./controller/webui"
    ],
    "todo-tree.filtering.excludeGlobs": [
        "**/monaco/**"
    ]
}